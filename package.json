{
  "name": "@tjdrbs205/config-client",
  "version": "1.0.2",
  "description": "A lightweight Config Client compatible with Spring Cloud Config Server",
  "keywords": [
    "config",
    "client",
    "spring-cloud-config",
    "configuration",
    "config-server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tjdrbs205/Config_Client_Private.git"
  },
  "license": "MIT",
  "author": "tjdrbs205",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.9.3",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
